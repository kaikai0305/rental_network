<div class="container">
   <div class="row" >
     <div class="col-xs-4">
        <h1>部屋編集</h1>
        <%= form_with model: @lending, url: admin_lending_path, method: :patch do |f| %>


        <h4>貸出物名</h4>
           <%= f.text_field :name %>
         <h4>個数</h4>
           <%= f.text_field :quantity %>
        <h4>貸出物説明</h4>
           <%= f.text_area :caption %>

        <br>
        <br>

        <%= f.submit '変更を保存', class:"btn btn-primary" %>

        <% end %>
     </div>
  </div>
</div>